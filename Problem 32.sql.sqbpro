FROM customers
INNER JOIN orders on customers.customerid = orders.customerid
INNER JOIN order_details on orders.orderid = order_details.orderid
WHERE orderdate &gt; '20160101' AND orderdate &lt; '20170101'
ORDER BY TotalOrderAmount DESC;</sql><current_tab id="0"/></tab_sql></sqlb_project>
